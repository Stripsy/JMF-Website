<div class="container">
  <h2>Participants : {{this.Evenements.nom}}</h2>
  {{#if messages.deleteparticipant}}
  <div class="alert alert-success" role="alert">
    {{messages.deleteparticipant}}
  </div>
  {{/if}}
  <table class="table table-striped table-dark">
    <thead>
      <tr>
        <th scope="col">Nom</th>
        <th scope="col">Prenom</th>
        <th scope="col">Mail</th>
        <th scope="col">Telephone</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody>
      {{#each user}}
      <tr>
        <th scope="row">{{this.nom}}</th>
        <td>{{this.prenom}}</td>
        <td>{{this.mail}}</td>
        <td>{{this.telephone}}</td>
        <td>
          <a href="/admin/evenements/participants/delete/{{this.Evenements.idevenement}}"><button type="button"
              class="btn btn-danger">Supprimer</button></a>
        </td>
      </tr>
      {{/each}}
      <tr></tr>

    </tbody>
  </table>
</div>